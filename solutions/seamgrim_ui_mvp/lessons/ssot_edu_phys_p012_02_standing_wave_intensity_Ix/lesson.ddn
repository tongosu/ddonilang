
(매마디)마다 {
  // 출력은 숫자만. (x, I) 순서.

  A <- 1.0.
  k <- 6.283185307.   // 2*pi

  x_min <- 0.
  x_max <- 1.
  dx <- 0.005.

  x목록 <- (x_min, x_max, dx) 범위.

  (x) x목록에 대해: {
    th <- (k=k, x=x) (#ascii) 수식{ k*x }.
    s <- ((th) sin).
    amp <- (2*A) * s.
    I <- amp * amp.

    보임 {
      x: x.
      I: I.
    }.
  }.
}.
