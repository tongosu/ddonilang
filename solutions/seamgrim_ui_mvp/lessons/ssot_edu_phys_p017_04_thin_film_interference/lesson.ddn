
(매마디)마다 {
  // 출력은 숫자만. (t, I) 순서.

  pi <- 3.1415926535.

  n <- 1.33.
  lam <- 1.0.
  I0 <- 1.0.

  t_min <- 0.
  t_max <- 2.0.
  dt <- 0.002.

  t목록 <- (t_min, t_max, dt) 범위.

  (t) t목록에 대해: {
    delta <- 4 * pi * n * t / lam.
    c <- (delta) cos.
    I <- 0.5 * I0 * (1 + c).

    보임 {
      t: t.
      I: I.
    }.
  }.
}.
