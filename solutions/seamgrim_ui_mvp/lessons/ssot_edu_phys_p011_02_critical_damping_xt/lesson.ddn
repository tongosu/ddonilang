
(매마디)마다 {
  // 출력은 숫자만. (t, x) 순서.
  // 모델: m x'' + c x' + k x = 0  (m=1, k=4)

  m <- 1.
  k <- 4.
  c <- 4.0.

  dt <- 0.01.
  t_max <- 10.

  x <- 1.
  v <- 0.

  t목록 <- (0, t_max, dt) 범위.

  (t) t목록에 대해: {
    // 현재 상태 출력
    보임 {
      t: t.
      x: x.
    }.

    // 다음 상태로 1스텝 진행(세미-임플리시트 Euler)
    a <- (-(k*x) - (c*v)) / m.
    v <- v + (a*dt).
    x <- x + (v*dt).
  }.
}.
