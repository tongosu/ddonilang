#이름: E014-04 최저임금 w_min과 초과공급(실업) U(w_min)
#설명: U = max(0, Ls(w_min) - Ld(w_min)).
// 출력은 숫자만. (w_min, U) 순서.

a <- 100.
b <- 2.
c <- 10.
d <- 1.5.

wmin_min <- 0.
wmin_max <- 60.
dw <- 1.

w목록 <- (wmin_min, wmin_max, dw) 범위.

(wmin) w목록에 대해: {
  Ld <- a - b*wmin.
  { Ld < 0 }인것 일때 {
    Ld <- 0.
  }.
  Ls <- c + d*wmin.
  { Ls < 0 }인것 일때 {
    Ls <- 0.
  }.
  U <- Ls - Ld.
  { U < 0 }인것 일때 {
    U <- 0.
  }.
  wmin 보여주기.
  U 보여주기.
}.
