
(매마디)마다 {
  // 출력은 숫자만. (e, NX) 순서.

  e0 <- 1200.
  X0 <- 50.
  M0 <- 55.

  x1 <- 0.03.     // X의 환율 민감도
  m1 <- 0.02.     // M의 환율 민감도

  e_min <- 800.
  e_max <- 1600.
  de <- 10.

  e목록 <- (e_min, e_max, de) 범위.

  (e) e목록에 대해: {
    X <- X0 + x1*(e - e0).
    M <- M0 - m1*(e - e0).

    NX <- X - M.

    보임 {
      e: e.
      NX: NX.
    }.
  }.
}.
