
(매마디)마다 {
  a <- 100.
  b <- 2.
  c <- -20.
  d <- 1.5.

  tau_min <- 0.
  tau_max <- 40.
  dtau <- 1.

  tau목록 <- (tau_min, tau_max, dtau) 범위.

  (tau) tau목록에 대해: {
    분모 <- b + d.
    p_p <- (a - c - (b * tau)) / 분모.
    { p_p < 0 }인것 일때 {
      p_p <- 0.
    }.
    p_c <- p_p + tau.

    보임 {
      tau: tau.
      p_c: p_c.
    }.
  }.
}.
