채비: {
  a:수 <- 2. // 범위(-5, 5, 0.1)
  b:수 <- 1. // 범위(-10, 10, 0.1)
  x_start:수 <- -4. // 범위(-20, 20, 0.5)
  x_end:수 <- 4. // 범위(-20, 20, 0.5)
  dx:수 <- 0.2. // 범위(0.05, 2, 0.05)
}.

(매마디)마다 {
  a <- 2.
  b <- 1.
  x_start <- -4.
  x_end <- 4.
  dx <- 0.2.

  x <- x_start.
  { x <= x_end }인것 동안: {
    y <- a * x + b.
    보임 {
      x: x.
      y: y.
    }.
    x <- x + dx.
  }.
}.
