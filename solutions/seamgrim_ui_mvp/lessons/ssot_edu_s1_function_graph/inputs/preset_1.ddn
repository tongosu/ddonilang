#이름: ssot_edu_s1_function_graph_preset_1
#설명: 절편이 음수인 일차함수 스캔
채비: {
  a:수 <- 2.2. // 범위(-5, 5, 0.1)
  b:수 <- -3.0. // 범위(-10, 10, 0.1)
  x_min:수 <- -5. // 범위(-20, 0, 0.5)
  x_max:수 <- 5. // 범위(0, 20, 0.5)
  dx:수 <- 0.25. // 범위(0.05, 2, 0.05)
}.

(매마디)마다 {
  a <- 2.2.
  b <- -3.0.
  x_min <- -5.
  x_max <- 5.
  dx <- 0.25.

  x <- x_min.
  { x <= x_max }인것 동안: {
    y <- a * x + b.
    보임 {
      x: x.
      y: y.
    }.
    x <- x + dx.
  }.
}.
