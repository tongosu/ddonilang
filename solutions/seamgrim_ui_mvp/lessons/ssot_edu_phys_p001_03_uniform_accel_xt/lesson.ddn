
(매마디)마다 {
  // 출력은 숫자만. (t, x) 순서.

  살림.x0 <- 0.
  살림.v0 <- 0.
  살림.a <- 1.
  살림.dt <- 0.1.
  살림.t_max <- 10.

  살림.t <- 0.

  { 살림.t <= 살림.t_max }인것 동안: {
    살림.x <- (x0=살림.x0, v0=살림.v0, a=살림.a, t=살림.t) (#ascii) 수식{ x0 + v0*t + (a*t^2)/2 }.

    // (t, x)
    보임 {
      값1: 살림.t.
      값2: 살림.x.
    }.

    살림.t <- 살림.t + 살림.dt.
  }.
}.
