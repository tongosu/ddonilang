#이름: high_math_quadratic
#설명: y = a*x^2 + b*x + c (상태 기반 시뮬)
#control: a:수=1 [-3..3] step=0.1; b:수=-2 [-8..8] step=0.1; c:수=-3 [-20..20] step=0.1; x_start:수=-4 [-20..20] step=0.5; x_end:수=4 [-20..20] step=0.5; dx:수=0.1 [0.02..1] step=0.02

(매마디)마다 {
  a <- 1.
  b <- -2.
  c <- -3.
  x_start <- -4.
  x_end <- 4.
  dx <- 0.1.

  x <- x_start.
  { x <= x_end }인것 동안: {
    y <- a * x * x + b * x + c.
    보임 {
      x: x.
      y: y.
    }.
    x <- x + dx.
  }.
}.
