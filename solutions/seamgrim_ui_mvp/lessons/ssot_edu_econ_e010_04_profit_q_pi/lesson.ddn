
(매마디)마다 {
  a <- 100.
  b <- 1.

  F <- 200.
  c <- 20.
  d <- 0.1.

  Q_min <- 0.
  Q_max <- 140.
  dQ <- 1.

  Q목록 <- (Q_min, Q_max, dQ) 범위.

  (Q) Q목록에 대해: {
    P <- a - (b * Q).
    { P < 0 }인것 일때 {
      P <- 0.
    }.
    TR <- P * Q.
    TC <- F + (c * Q) + (d * Q * Q).
    PI <- TR - TC.

    보임 {
      Q: Q.
      PI: PI.
    }.
  }.
}.
