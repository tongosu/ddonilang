
(매마디)마다 {
  a <- 100.
  b <- 1.

  c_min <- 0.
  c_max <- 90.
  dc <- 2.

  c목록 <- (c_min, c_max, dc) 범위.

  (c) c목록에 대해: {
    DWL <- 0.

    { c < a }인것 일때 {

      Qc <- (a - c) / b.
      Qm <- (a - c) / (2 * b).
      Pm <- (a + c) / 2.

      DWL <- 0.5 * (Pm - c) * (Qc - Qm).

    }.
    보임 {
      c: c.
      DWL: DWL.
    }.
  }.
}.
