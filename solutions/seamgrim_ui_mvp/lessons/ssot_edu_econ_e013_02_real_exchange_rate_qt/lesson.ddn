
(매마디)마다 {
  // 출력은 숫자만. (t, q) 순서.

  e <- 1200.          // 명목환율(예: 원/달러)
  P_dom0 <- 100.
  P_for0 <- 100.

  pi_dom <- 0.03.     // 국내 물가상승률(연/기 단위는 해석에 맞춰)
  pi_for <- 0.02.

  dt <- 1.
  t_max <- 50.
  t목록 <- (0, t_max, dt) 범위.

  P_dom <- P_dom0.
  P_for <- P_for0.

  (t) t목록에 대해: {
    q <- e * (P_for / P_dom).

    보임 {
      t: t.
      q: q.
    }.

    P_dom <- P_dom * (1 + pi_dom).
    P_for <- P_for * (1 + pi_for).
  }.
}.
