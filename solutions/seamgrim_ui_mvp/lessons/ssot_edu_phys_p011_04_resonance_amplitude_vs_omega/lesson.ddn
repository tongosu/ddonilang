
(매마디)마다 {
  // 출력은 숫자만. (omega, A) 순서.
  // 설정: m=1, k=4(자연각진동수=2), c=0.5, F0=1.

  m <- 1.
  k <- 4.
  c <- 0.5.
  F0 <- 1.

  w_min <- 0.
  w_max <- 4.
  dw <- 0.02.

  w목록 <- (w_min, w_max, dw) 범위.

  (w) w목록에 대해: {
    d <- k - (m*w*w).
    p <- d * d.
    q <- (c*w) * (c*w).
    u <- p + q.
    den <- ((u) sqrt).
    A <- F0 / den.

    보임 {
      w: w.
      A: A.
    }.
  }.
}.
