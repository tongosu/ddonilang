#이름: seamgrim_ui_mvp_moving_dot_sample_strict
#설명: strict 모드용. 프레임수로 점이 대각선 이동(모듈로 wrap).

그릇채비: {
  프레임수:수 <- 0.
}.

매마디:움직씨 = {
  보개_그림판_가로 <- 200.
  보개_그림판_세로 <- 120.

  rect <- ("결", "#보개/2D.Rect", "x", (20 + (프레임수 % 160)), "y", (20 + ((프레임수 * 2) % 80)), "w", 6, "h", 6, "채움색", "#f59e0bff") 짝맞춤.
  보개_그림판_목록 <- (rect) 차림.

  프레임수 <- (프레임수 + 1).
}.
