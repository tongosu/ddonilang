채비: {
  x0:수 <- 0. // 범위(-20, 20, 0.5)
  v0:수 <- 1. // 범위(-20, 20, 0.5)
  a:수 <- 0.2. // 범위(-10, 10, 0.1)
  dt:수 <- 0.1. // 범위(0.02, 1, 0.01)
  t_max:수 <- 8. // 범위(1, 30, 0.5)
}.

x0 <- 0.
v0 <- 1.
a <- 0.2.
dt <- 0.1.
t_max <- 8.

(시작)할때 {
  t <- 0.
  x <- x0.
  v <- v0.
}.

(매마디)마다 {
  { t <= t_max }인것 일때 {
    v <- v + a * dt.
    x <- x + v * dt.
    t <- t + dt.

    t 보여주기.
    x 보여주기.
    v 보여주기.
  }.
}.
