채비: {
  y0:수 <- 100. // 범위(10, 500, 1)
  g:수 <- 0.03. // 범위(-0.05, 0.2, 0.005)
  dt:수 <- 1. // 범위(1, 1, 1)
  t_max:수 <- 20. // 범위(5, 80, 1)
}.

y0 <- 100.
g <- 0.03.
dt <- 1.
t_max <- 20.

(시작)할때 {
  t <- 0.
  y <- y0.
}.

(매마디)마다 {
  { t <= t_max }인것 일때 {
    y <- y * (1 + g * dt).
    t <- t + dt.

    t 보여주기.
    y 보여주기.
  }.
}.
