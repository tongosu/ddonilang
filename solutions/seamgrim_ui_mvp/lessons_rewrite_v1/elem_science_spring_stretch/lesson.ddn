채비: {
  x0:수 <- 1. // 범위(-5, 5, 0.1)
  v0:수 <- 0. // 범위(-5, 5, 0.1)
  k:수 <- 1. // 범위(0.1, 8, 0.1)
  c:수 <- 0.1. // 범위(0, 2, 0.05)
  dt:수 <- 0.02. // 범위(0.005, 0.1, 0.005)
  t_max:수 <- 8. // 범위(1, 30, 0.5)
}.

x0 <- 1.
v0 <- 0.
k <- 1.
c <- 0.1.
dt <- 0.02.
t_max <- 8.

(시작)할때 {
  t <- 0.
  x <- x0.
  v <- v0.
}.

(매마디)마다 {
  { t <= t_max }인것 일때 {
    a <- 0 - k * x - c * v.
    v <- v + a * dt.
    x <- x + v * dt.
    t <- t + dt.

    t 보여주기.
    x 보여주기.
    v 보여주기.
  }.
}.
