보개_그림판_가로 <- 128.
보개_그림판_세로 <- 96.
보개_바탕색 <- "#0b1020ff".

x1 <- 10.
x2 <- 20.
x3 <- 30.
x4 <- 40.
x5 <- 50.

세율 <- 0.5.
총부 <- x1 + x2 + x3 + x4 + x5.
재분배 <- (총부 * 세율) / 5.

y1 <- x1 * (1 - 세율) + 재분배.
y2 <- x2 * (1 - 세율) + 재분배.
y3 <- x3 * (1 - 세율) + 재분배.
y4 <- x4 * (1 - 세율) + 재분배.
y5 <- x5 * (1 - 세율) + 재분배.

부목록 <- [y1, y2, y3, y4, y5].
p90 <- (부목록, 0.9, "선형보간") 분위수.

