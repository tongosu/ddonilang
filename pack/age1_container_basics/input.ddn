살림.정렬됨 <- (살림.추가됨, "길이") 정렬.
살림.변환됨 <- (살림.조각들, "길이") 변환.
살림.최대길이 <- (살림.변환됨, 0, "max") 합치기.
살림.합친 <- (살림.조각들, "-") 붙이기.
살림.길이 <- (살림.조각들) 길이.
살림.첫 <- (살림.조각들) 첫번째.
살림.마지막 <- (살림.조각들) 마지막.
살림.길이 보여주기.
